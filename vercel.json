{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/eos2/scheduled-checks?check=todos",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/eos2/scheduled-checks?check=rocks",
      "schedule": "0 9 * * 1"
    },
    {
      "path": "/api/eos2/scheduled-checks?check=metrics",
      "schedule": "0 9 * * 1"
    },
    {
      "path": "/api/eos2/weekly-digest",
      "schedule": "0 8 * * 1"
    }
  ]
}
