{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/traction/scheduled-checks?check=todos",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/traction/scheduled-checks?check=rocks",
      "schedule": "0 9 * * 1"
    },
    {
      "path": "/api/traction/scheduled-checks?check=metrics",
      "schedule": "0 9 * * 1"
    },
    {
      "path": "/api/traction/weekly-digest",
      "schedule": "0 8 * * 1"
    }
  ]
}
